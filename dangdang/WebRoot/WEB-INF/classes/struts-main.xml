    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="name" namespace="/main" extends="dang-default">
		<action name="category" class="com.dang.actions.main.CategoryAction">
			<result>/main/category.jsp</result>
		</action>
		<action name="recommend" class="com.dang.actions.main.RecommendAction">
			<result>/main/recommend.jsp</result>
